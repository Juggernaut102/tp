@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedEduDex" as VersionedEduDex MODEL_COLOR
end box

[-> Model : undoEduDex()
activate Model

Model -> VersionedEduDex : undo()
activate VersionedEduDex

VersionedEduDex -> VersionedEduDex :resetData(ReadOnlyEduDex)
VersionedEduDex --> Model :
deactivate VersionedEduDex

[<-- Model
deactivate Model

@enduml
